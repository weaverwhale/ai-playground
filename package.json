{
  "name": "o-playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "tsx watch backend/server.ts",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build:frontend": "vite build",
    "build:backend": "tsc -p tsconfig.prod.json",
    "build": "npm run clean && npm run build:frontend && npm run build:backend",
    "lint": "eslint .",
    "backend:serve": "tsx dist/backend/server.js",
    "prod": "npm run build && npm run backend:serve",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "marked": "^15.0.1",
    "mermaid": "^11.4.1",
    "openai": "^4.77.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "regression": "^2.0.1",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/regression": "^2.0.6",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "sass": "^1.82.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
